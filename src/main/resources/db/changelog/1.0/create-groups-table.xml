<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                                       http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <changeSet id="create-table-groups" author="Evgenii Tsarin">
        <createTable tableName="univercity_groups">
            <column
                    name="id"
                    type="integer"
                    autoIncrement="true">
                <constraints
                        primaryKey="true"
                        nullable="false"/>
            </column>

            <column
                name="group_number"
                type="varchar(50)">
                <constraints
                    nullable="false"/>
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>
